print("rules:\n1=stone\n2=paper\n3=scissor")
a=int(input("enter a number between 1-3:"))
b=int(input("enter a number between 1-3:"))
if(a==1 and b==2):
   print("user 2 is winner")
elif(a==2 and b==3):
   print("user 2 is winner")
elif(a==3 and b==1):
   print("user 2 is winner")
elif(a==b):
   print("draw")
else:
   print("user 1 is winner")
